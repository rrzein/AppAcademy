<!DOCTYPE html>
<html>
<head>
  <title>Newsstand</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% flash[:notices].try(:each) do |msg| %>
		<%= msg %>
	<% end %>
	<% flash[:errors].try(:each) do |msg| %>
		<%= msg %>
	<% end %>

<%= yield %>

</body>
</html>
